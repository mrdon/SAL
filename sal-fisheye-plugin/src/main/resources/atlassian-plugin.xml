<atlassian-plugin key="com.atlassian.sal.fisheye" name="${project.name}" system="true" pluginsVersion="2">
    <plugin-info>
        <description>
            A plugin that links FishEye to other Atlassian applications    
        </description>
        <vendor name="Atlassian Software Systems" url="http://www.atlassian.com"/>
        <version>${pom.version}</version>
    </plugin-info>

	<spring key="pluginSettingsFactory" class="com.atlassian.sal.fisheye.pluginsettings.FisheyePluginSettingsFactory"/>
    <spring key="componentLocator" class="com.atlassian.sal.core.component.DefaultComponentLocator"/>
    <spring key="pluginUpgradeManager" class="com.atlassian.sal.core.upgrade.DefaultPluginUpgradeManager"/>
    <spring key="certificateFactory" class="com.atlassian.sal.fisheye.trusted.FisheyeCertificateFactory"/>
    <spring key="userManager" class="com.atlassian.sal.fisheye.user.DefaultUserManager"/>
    <spring key="applicationProperties" class="com.atlassian.sal.fisheye.FisheyeApplicationProperties"/>
    <spring key="licenseHandler" class="com.atlassian.sal.fisheye.license.FisheyeLicenseHandler" />
    <spring key="lifecycleManager" class="com.atlassian.sal.fisheye.lifecycle.FisheyeLifecycleManager" />
    <spring key="readWriteTransactionTemplate" class="com.atlassian.sal.core.transaction.DefaultTransactionTemplate" />
    <spring key="cenquaSearchProvider" class="com.atlassian.sal.fisheye.search.CenquaSearchProvider" />
    <spring key="searchQueryParser" class="com.atlassian.sal.core.search.query.DefaultSearchQueryParser" />
    <spring key="pluginScheduler" class="com.atlassian.sal.core.scheduling.TimerPluginScheduler" />
    <spring key="requestFactory" class="com.atlassian.sal.core.net.HttpClientRequestFactory"/>
    <spring key="i18nResolver" class="com.atlassian.sal.fisheye.message.NoOpI18nResolver"/>
    <spring key="fisheyeProjectManager" class="com.atlassian.sal.fisheye.project.FisheyeProjectManager" />

</atlassian-plugin>
