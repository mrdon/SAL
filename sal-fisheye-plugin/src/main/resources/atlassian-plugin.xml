<atlassian-plugin key="com.atlassian.sal.fisheye" name="${project.name}" system="true">
    <plugin-info>
        <description>
            A plugin that links FishEye to other Atlassian applications    
        </description>
        <vendor name="Atlassian Software Systems" url="http://www.atlassian.com"/>
        <version>${pom.version}</version>
    </plugin-info>

	<spring key="pluginSettingsFactory" class="com.atlassian.sal.fisheye.pluginsettings.FisheyePluginSettingsFactory"/>
    <spring key="componentLocator" class="com.atlassian.sal.spring.component.SpringComponentLocator"/>
    <spring key="pluginUpgradeManager" class="com.atlassian.sal.api.upgrade.DefaultPluginUpgradeManager"/>
    <spring key="certificateFactory" class="com.atlassian.sal.fisheye.trusted.FisheyeCertificateFactory"/>
    <spring key="userManager" class="com.atlassian.sal.fisheye.user.DefaultUserManager"/>
    <spring key="applicationProperties" class="com.atlassian.sal.fisheye.FisheyeApplicationProperties"/>
    <spring key="licenseHandler" class="com.atlassian.sal.fisheye.license.FisheyeLicenseHandler" />
    <spring key="lifecycleManager" class="com.atlassian.sal.fisheye.lifecycle.FisheyeLifecycleManager" />
    <spring key="readWriteTransactionTemplate" class="com.atlassian.sal.spring.transaction.ReadWriteTransactionTemplate" />
    <spring key="cenquaSearchProvider" class="com.atlassian.sal.fisheye.search.CenquaSearchProvider" />
    <spring key="searchQueryParser" class="com.atlassian.sal.api.search.query.DefaultSearchQueryParser" />
    <spring key="pluginScheduler" class="com.atlassian.sal.fisheye.scheduling.FisheyePluginScheduler" />
    <spring key="requestFactory" class="com.atlassian.sal.api.net.HttpClientRequestFactory"/>

</atlassian-plugin>
