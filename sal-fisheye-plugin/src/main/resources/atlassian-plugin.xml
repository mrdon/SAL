<atlassian-plugin key="com.atlassian.sal.fisheye" name="${project.name}" system="true" pluginsVersion="2">
    <plugin-info>
        <description>
            A plugin that provides an access layer for the underlying application.
        </description>
        <vendor name="Atlassian Software Systems" url="http://www.atlassian.com"/>
        <version>${pom.version}</version>
    </plugin-info>

	<component key="pluginSettingsFactory" class="com.atlassian.sal.fisheye.pluginsettings.FisheyePluginSettingsFactory" public="true">
        <interface>com.atlassian.sal.api.pluginsettings.PluginSettingsFactory</interface>
    </component>
    <component key="componentLocator" class="com.atlassian.sal.core.component.DefaultComponentLocator" public="true">
        <interface>com.atlassian.sal.api.component.ComponentLocator</interface>
    </component>
    <component key="certificateFactory" class="com.atlassian.sal.fisheye.trusted.FisheyeCertificateFactory" public="true">
        <interface>com.atlassian.sal.core.trusted.CertificateFactory</interface>
    </component>
    <component key="salUserManager" class="com.atlassian.sal.fisheye.user.DefaultUserManager" public="true">
        <interface>com.atlassian.sal.api.user.UserManager</interface>
    </component>
    <component key="applicationProperties" class="com.atlassian.sal.fisheye.FisheyeApplicationProperties" public="true">
        <interface>com.atlassian.sal.api.ApplicationProperties</interface>
    </component>
    <component key="licenseHandler" class="com.atlassian.sal.fisheye.license.FisheyeLicenseHandler" public="true" >
        <interface>com.atlassian.sal.api.license.LicenseHandler</interface>
    </component>
    <component key="transactionTemplate" class="com.atlassian.sal.core.transaction.HostContextTransactionTemplate" public="true" >
        <interface>com.atlassian.sal.api.transaction.TransactionTemplate</interface>
    </component>
    <component key="searchProvider" class="com.atlassian.sal.fisheye.search.CenquaSearchProvider" public="true" >
        <interface>com.atlassian.sal.api.search.SearchProvider</interface>
    </component>
    <component key="searchQueryParser" class="com.atlassian.sal.core.search.query.DefaultSearchQueryParser" public="true" >
        <interface>com.atlassian.sal.api.search.query.SearchQueryParser</interface>
    </component>
    <component key="pluginScheduler" class="com.atlassian.sal.core.scheduling.TimerPluginScheduler" public="true" >
        <interface>com.atlassian.sal.api.scheduling.PluginScheduler</interface>
    </component>
    <component key="requestFactory" class="com.atlassian.sal.core.net.HttpClientRequestFactory" public="true">
        <interface>com.atlassian.sal.api.net.RequestFactory</interface>
    </component>
    <component key="i18nResolver" class="com.atlassian.sal.fisheye.message.NoOpI18nResolver" public="true">
        <interface>com.atlassian.sal.api.message.I18nResolver</interface>
    </component>
    <component key="projectManager" class="com.atlassian.sal.fisheye.project.FisheyeProjectManager"  public="true">
        <interface>com.atlassian.sal.api.project.ProjectManager</interface>
    </component>

</atlassian-plugin>
