<atlassian-plugin key="com.atlassian.sal.fisheye" name="${project.name}" system="true" pluginsVersion="2">
    <plugin-info>
        <description>
            A plugin that links FishEye to other Atlassian applications    
        </description>
        <vendor name="Atlassian Software Systems" url="http://www.atlassian.com"/>
        <version>${pom.version}</version>
    </plugin-info>

	<component key="pluginSettingsFactory" class="com.atlassian.sal.fisheye.pluginsettings.FisheyePluginSettingsFactory" public="true"/>
    <component key="componentLocator" class="com.atlassian.sal.core.component.DefaultComponentLocator" public="true"/>
    <component key="pluginUpgradeManager" class="com.atlassian.sal.core.upgrade.DefaultPluginUpgradeManager" public="true"/>
    <component key="certificateFactory" class="com.atlassian.sal.fisheye.trusted.FisheyeCertificateFactory" public="true"/>
    <component key="userManager" class="com.atlassian.sal.fisheye.user.DefaultUserManager" public="true"/>
    <component key="applicationProperties" class="com.atlassian.sal.fisheye.FisheyeApplicationProperties" public="true"/>
    <component key="licenseHandler" class="com.atlassian.sal.fisheye.license.FisheyeLicenseHandler" public="true" />
    <component key="lifecycleManager" class="com.atlassian.sal.fisheye.lifecycle.FisheyeLifecycleManager" public="true" />
    <component key="readWriteTransactionTemplate" class="com.atlassian.sal.core.transaction.DefaultTransactionTemplate" public="true" />
    <component key="cenquaSearchProvider" class="com.atlassian.sal.fisheye.search.CenquaSearchProvider" public="true" />
    <component key="searchQueryParser" class="com.atlassian.sal.core.search.query.DefaultSearchQueryParser" public="true" />
    <component key="pluginScheduler" class="com.atlassian.sal.core.scheduling.TimerPluginScheduler" public="true" />
    <component key="requestFactory" class="com.atlassian.sal.core.net.HttpClientRequestFactory" public="true"/>
    <component key="i18nResolver" class="com.atlassian.sal.fisheye.message.NoOpI18nResolver" public="true"/>
    <component key="fisheyeProjectManager" class="com.atlassian.sal.fisheye.project.FisheyeProjectManager"  public="true"/>

</atlassian-plugin>
